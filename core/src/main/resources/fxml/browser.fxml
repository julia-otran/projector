<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<VBox id="RootBox" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dev.juhouse.projector.forms.controllers.projection.BrowserController">
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
   <Pane fx:id="projectionControlPane" VBox.vgrow="NEVER" />
   <TextField fx:id="adddressTextField" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" onKeyPressed="#onAddressFiledKeyPress">
      <VBox.margin>
         <Insets top="10.0" />
      </VBox.margin>
   </TextField>
   <ScrollPane fx:id="browserPane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="200.0" VBox.vgrow="ALWAYS">
      <VBox.margin>
         <Insets bottom="10.0" top="10.0" />
      </VBox.margin>
   </ScrollPane>
   <ProgressBar fx:id="browserProgressBar" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="5.0" progress="0.0" />
</VBox>
