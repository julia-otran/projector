<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.*?>
<VBox id="RootBox" prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1"
      xmlns="http://javafx.com/javafx/8"
      fx:controller="us.guihouse.projector.forms.controllers.projection.TextController">
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
   </padding>
   <HBox maxWidth="1.7976931348623157E308" VBox.vgrow="NEVER">
      <Button fx:id="beginProjectionButton" maxWidth="1.7976931348623157E308" mnemonicParsing="false"
              onAction="#onBeginProjection" text="Projetar na Tela" HBox.hgrow="ALWAYS"/>
      <Button fx:id="endProjectionButton" maxWidth="1.7976931348623157E308" mnemonicParsing="false"
              onAction="#onEndProjection" text="Remover da Tela (ESC)" HBox.hgrow="ALWAYS"/>
   </HBox>
   <Label text="Digite o Texto:">
      <VBox.margin>
         <Insets top="10.0"/>
      </VBox.margin>
   </Label>
   <TextField fx:id="projectionText">
      <VBox.margin>
         <Insets/>
      </VBox.margin>
   </TextField>
</VBox>
